{
    "name": "leinonen/yii2-algolia",
    "description": "Algolia bridge for Yii2",
    "type": "yii2-extension",
    "keywords": ["yii2", "algolia", "search"],
    "license": "MIT",
    "authors": [
        {
            "name": "Juuso Leinonen",
            "email": "leinonen.juuso@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "^2.0",
        "algolia/algoliasearch-client-php": "^1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*",
        "yiisoft/yii2-dev": "^2.0",
        "mockery/mockery": "^0.9.4"
    },
    "autoload": {
        "psr-4": {
            "leinonen\\Yii2Algolia\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "leinonen\\Yii2Algolia\\Tests\\": "tests/"
        }
    }
}
